<resources>
    <declare-styleable name="CheckableRelativeLayout">
        <attr name="checkableChildId" format="reference" />
    </declare-styleable>

    <attr name="max" format="integer" />
    <attr name="min" format="integer" />
    <attr name="ratio" format="integer" />
    <attr name="step" format="integer" />
    <attr name="nullsummary" format="string"/>

    <attr name="summaryp" format="string"/>
    <attr name="summaryz" format="string"/>
    <attr name="summaryn" format="string"/>

    <attr name="seekBarDialogPreferenceStyle" format="reference"/>
    <declare-styleable name="ExtSeekBarDialogPreference">
        <attr name="max"/>
        <attr name="min"/>
        <attr name="ratio"/>
        <attr name="step"/>

        <attr name="summaryp"/>
        <attr name="summaryz"/>
        <attr name="summaryn"/>
    </declare-styleable>

    <attr name="seekBarPreferenceStyle" format="reference"/>
    <declare-styleable name="ExtSeekBarPreference">
        <attr name="max"/>
        <attr name="min"/>
        <attr name="ratio"/>
        <attr name="step"/>

        <attr name="summaryp"/>
        <attr name="summaryz"/>
        <attr name="summaryn"/>
    </declare-styleable>

    <attr name="extEditTextPreferenceStyle" format="reference"/>
    <declare-styleable name="ExtEditTextPreference">
        <attr name="nullsummary"/>
    </declare-styleable>
</resources>
